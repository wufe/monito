{
  "name": "monito.web",
  "version": "1.0.0",
  "author": "wufe <simone.bembi@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wufe/monito/tree/master/Presentation/Monito.Web"
  },
  "scripts": {
    "watch:client": "parcel watch -t browser web-client/index.html --public-url /static --out-dir wwwroot/dist/static --hmr-port 44413 --no-cache",
    "build:client": "parcel build -t browser web-client/index.html --public-url /static --out-dir release/wwwroot/dist/static --no-cache",
    "watch:server:restore": "dotnet restore",
    "watch:server:clean": "dotnet clean",
    "watch:server:run": "dotnet watch run",
    "watch:server": "run-s watch:server:restore watch:server:clean watch:server:run",
    "publish:server": "dotnet publish --configuration Release --output release --self-contained --runtime linux-x64",
    "dev": "run-p watch:client watch:server",
    "prod": "run-s publish:server build:client"
  },
  "devDependencies": {
    "@microsoft/signalr": "^3.1.0",
    "@types/react": "^16.9.17",
    "@types/react-custom-scrollbars": "^4.0.6",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-virtualized": "^9.21.7",
    "@types/redux": "^3.6.0",
    "@types/uuid": "^3.4.6",
    "@wufe/floor-animation": "^1.0.2",
    "axios": "^0.19.0",
    "connected-react-router": "^6.6.1",
    "cookieconsent": "^3.1.1",
    "core-js": "^3.6.1",
    "cssnano": "^4.1.10",
    "moment": "^2.29.4",
    "nord": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "posthtml-expressions": "^1.1.1",
    "react": "^16.12.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.24.0",
    "typescript": "^3.7.4",
    "uuid": "^3.3.3"
  }
}
